<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Onliner tests" parallel="methods">
    <test name="Onliner tests on chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="by.it_academy.tests.ui_test.OnlinerTest"/>
        </classes>
    </test>
    <test name="Onliner tests on firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="by.it_academy.tests.ui_test.OnlinerTest"/>
        </classes>
    </test>
    <test name="Onliner tests on edge">
        <parameter name="browser" value="microsoftedge"/>
        <classes>
            <class name="by.it_academy.tests.ui_test.OnlinerTest"/>
        </classes>
    </test>
    <test name="API Onliner tests">
        <classes>
            <class name="by.it_academy.tests.api_tests.SushiveslaTest"/>
        </classes>
    </test>
</suite>